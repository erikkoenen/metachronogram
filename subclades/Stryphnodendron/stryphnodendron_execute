begin mrbayes;
	set autoclose=yes nowarnings=yes;
[	set usebeagle=yes beagledevice=cpu beagleprecision=double;]
[	set beaglescaling=dynamic beaglesse=yes;]
        execute Stryphnodendron_Simon_alignment.nex;
	lset nst=6 rates=gamma;
	constraint stryphno_clade hard = Microlobius_foetidus_Hugues2150_CEN422 Parapiptadenia_excelsa_Hugues2425_CEN431 Parapiptadenia_pterosperma_gb_trnLF Parapiptadenia_rigida_gb_trnLF Parapiptadenia_zehntneri_PereiraSilva3102_CEN585 Pityrocarpa_leucoxylon Pityrocarpa_moniliformis_Way2449_CEN521 Pityrocarpa_obliqua_Macqueen439_CEN437 Pseudopiptadenia_bahiana_Coradin8580_CEN997 Pseudopiptadenia_brenanii_Borges680_CEN654 Pseudopiptadenia_contorta_Queiroz15507_CEN999 Pseudopiptadenia_psilostachya_Simon1245_CEN581 Pseudopiptadenia_suaveolens Stryphnodendron_adstringens_VRS9_CEN599 Stryphnodendron_coriaceum_VRS21_CEN611 Stryphnodendron_cristalinae_VRS35_CEN623 Stryphnodendron_duckeanum_Simon1343_CEN582 Stryphnodendron_fissuratum_VRS19_CEN609 Stryphnodendron_foreroi_Simon1652A_CEN584 Stryphnodendron_gracile_VRS39_CEN625 Stryphnodendron_guianense_VRS15_CEN605 Stryphnodendron_heringeri_VRS34_CEN622 Stryphnodendron_microstachys Stryphnodendron_moricolor Stryphnodendron_obovatum_VRS10_CEN600 Stryphnodendron_occhionianum_Simon1597_CEN583 Stryphnodendron_paniculatum_VRS18_CEN608 Stryphnodendron_polyphyllum_VRS8_CEN598 Stryphnodendron_polystachyum_gb_trnLF Stryphnodendron_porcatum_gb Stryphnodendron_pulcherrimum_VRS13_CEN603 Stryphnodendron_pumilum_VRS38_CEN624 Stryphnodendron_racemiferum_VRS17_CEN607 Stryphnodendron_roseiflorum_VRS16_CEN606 Stryphnodendron_rotundifolium_villosum_VRS31_CEN620 Stryphnodendron_velutinum_VRS23_CEN613;
	constraint ingoid_mimosa hard = Inga_edulis Senegalia_nigrescens Mimosa_myriadenia Piptadenia_robusta;
	constraint ingroup hard = Microlobius_foetidus_Hugues2150_CEN422 Parapiptadenia_excelsa_Hugues2425_CEN431 Parapiptadenia_pterosperma_gb_trnLF Parapiptadenia_rigida_gb_trnLF Parapiptadenia_zehntneri_PereiraSilva3102_CEN585 Pityrocarpa_leucoxylon Pityrocarpa_moniliformis_Way2449_CEN521 Pityrocarpa_obliqua_Macqueen439_CEN437 Pseudopiptadenia_bahiana_Coradin8580_CEN997 Pseudopiptadenia_brenanii_Borges680_CEN654 Pseudopiptadenia_contorta_Queiroz15507_CEN999 Pseudopiptadenia_psilostachya_Simon1245_CEN581 Pseudopiptadenia_suaveolens Stryphnodendron_adstringens_VRS9_CEN599 Stryphnodendron_coriaceum_VRS21_CEN611 Stryphnodendron_cristalinae_VRS35_CEN623 Stryphnodendron_duckeanum_Simon1343_CEN582 Stryphnodendron_fissuratum_VRS19_CEN609 Stryphnodendron_foreroi_Simon1652A_CEN584 Stryphnodendron_gracile_VRS39_CEN625 Stryphnodendron_guianense_VRS15_CEN605 Stryphnodendron_heringeri_VRS34_CEN622 Stryphnodendron_microstachys Stryphnodendron_moricolor Stryphnodendron_obovatum_VRS10_CEN600 Stryphnodendron_occhionianum_Simon1597_CEN583 Stryphnodendron_paniculatum_VRS18_CEN608 Stryphnodendron_polyphyllum_VRS8_CEN598 Stryphnodendron_polystachyum_gb_trnLF Stryphnodendron_porcatum_gb Stryphnodendron_pulcherrimum_VRS13_CEN603 Stryphnodendron_pumilum_VRS38_CEN624 Stryphnodendron_racemiferum_VRS17_CEN607 Stryphnodendron_roseiflorum_VRS16_CEN606 Stryphnodendron_rotundifolium_villosum_VRS31_CEN620 Stryphnodendron_velutinum_VRS23_CEN613 Inga_edulis Senegalia_nigrescens Mimosa_myriadenia Piptadenia_robusta;
	calibrate ingroup=fixed(32);
	prset topologypr=constraints(stryphno_clade,ingoid_mimosa,ingroup);
	prset brlenspr=clock:uniform;
	prset clockvarpr=igr;
	showmodel;
	mcmc ngen=30000000 samplefreq=1000 file=stryphnodendron.nex1;
	mcmc file=stryphnodendron.nex2;
