begin mrbayes;
	set autoclose=yes nowarnings=yes;
[	set usebeagle=yes beagledevice=cpu beagleprecision=double;]
[	set beaglescaling=dynamic beaglesse=yes;]
        execute Calliandra_deSouza_alignment.nex;
	lset nst=6 rates=gamma;
	constraint afrocalliandra hard = Afrocalliandra_gilbertii Afrocalliandra_redacta;
	constraint calliandra hard = Calliandra_aeschynomenoides Calliandra_angustifolia Calliandra_asplenioides Calliandra_bahiana Calliandra_belizensis Calliandra_bella Calliandra_biflora Calliandra_blanchetii Calliandra_brenesii Calliandra_brevicaulis Calliandra_brevipes Calliandra_caeciliae Calliandra_californica Calliandra_calycina Calliandra_chilensis Calliandra_coccinea Calliandra_coriacea Calliandra_crassipes Calliandra_cruegeri Calliandra_cumbucana Calliandra_debilis Calliandra_depauperata Calliandra_dolichopoda Calliandra_dysantha Calliandra_eriophylla Calliandra_erubescens Calliandra_fasciculata Calliandra_feioana Calliandra_glomerulata Calliandra_goldmanii Calliandra_guildingii Calliandra_haematocephala Calliandra_haematomma Calliandra_harrisii Calliandra_hintonii Calliandra_hirsuta Calliandra_hirtiflora Calliandra_houstoniana Calliandra_humilis Calliandra_hygrophila Calliandra_iligna Calliandra_laevis Calliandra_lanata Calliandra_laxa Calliandra_leptopoda Calliandra_lintea Calliandra_longipes Calliandra_longipinna Calliandra_luetzelburgii Calliandra_macrocalyx Calliandra_magdalenae Calliandra_molinae Calliandra_mollissima Calliandra_mucugeana Calliandra_nebulosa Calliandra_palmeri Calliandra_parviflora Calliandra_parvifolia Calliandra_paterna Calliandra_pedicellata Calliandra_physocalyx Calliandra_pilgeriana Calliandra_pittieri Calliandra_purdiei Calliandra_purpurea Calliandra_renvoizeana Calliandra_rhodocephala Calliandra_riparia Calliandra_rubescens Calliandra_semisepulta Calliandra_sessilis Calliandra_silvicola Calliandra_sincorana Calliandra_spinosa Calliandra_spnovaBE Calliandra_spnovaMG Calliandra_spnovaSO Calliandra_surinamensis Calliandra_taxifolia Calliandra_tehuantepecensis Calliandra_tergemina Calliandra_trinervia Calliandra_tsugoides Calliandra_tumbeziana Calliandra_tweedii Calliandra_ulei Calliandra_umbellifera Calliandra_vaupesiana Calliandra_virgata Calliandra_viscidula;
	constraint calliandra_stem hard = Afrocalliandra_gilbertii Afrocalliandra_redacta Calliandra_aeschynomenoides Calliandra_angustifolia Calliandra_asplenioides Calliandra_bahiana Calliandra_belizensis Calliandra_bella Calliandra_biflora Calliandra_blanchetii Calliandra_brenesii Calliandra_brevicaulis Calliandra_brevipes Calliandra_caeciliae Calliandra_californica Calliandra_calycina Calliandra_chilensis Calliandra_coccinea Calliandra_coriacea Calliandra_crassipes Calliandra_cruegeri Calliandra_cumbucana Calliandra_debilis Calliandra_depauperata Calliandra_dolichopoda Calliandra_dysantha Calliandra_eriophylla Calliandra_erubescens Calliandra_fasciculata Calliandra_feioana Calliandra_glomerulata Calliandra_goldmanii Calliandra_guildingii Calliandra_haematocephala Calliandra_haematomma Calliandra_harrisii Calliandra_hintonii Calliandra_hirsuta Calliandra_hirtiflora Calliandra_houstoniana Calliandra_humilis Calliandra_hygrophila Calliandra_iligna Calliandra_laevis Calliandra_lanata Calliandra_laxa Calliandra_leptopoda Calliandra_lintea Calliandra_longipes Calliandra_longipinna Calliandra_luetzelburgii Calliandra_macrocalyx Calliandra_magdalenae Calliandra_molinae Calliandra_mollissima Calliandra_mucugeana Calliandra_nebulosa Calliandra_palmeri Calliandra_parviflora Calliandra_parvifolia Calliandra_paterna Calliandra_pedicellata Calliandra_physocalyx Calliandra_pilgeriana Calliandra_pittieri Calliandra_purdiei Calliandra_purpurea Calliandra_renvoizeana Calliandra_rhodocephala Calliandra_riparia Calliandra_rubescens Calliandra_semisepulta Calliandra_sessilis Calliandra_silvicola Calliandra_sincorana Calliandra_spinosa Calliandra_spnovaBE Calliandra_spnovaMG Calliandra_spnovaSO Calliandra_surinamensis Calliandra_taxifolia Calliandra_tehuantepecensis Calliandra_tergemina Calliandra_trinervia Calliandra_tsugoides Calliandra_tumbeziana Calliandra_tweedii Calliandra_ulei Calliandra_umbellifera Calliandra_vaupesiana Calliandra_virgata Calliandra_viscidula;
	calibrate calliandra_stem=fixed(16);
	prset topologypr=constraints(afrocalliandra,calliandra);
	prset brlenspr=clock:uniform;
	prset clockvarpr=igr;
	showmodel;
	mcmc ngen=30000000 samplefreq=1000 file=calliandra.nex1;
	mcmc file=calliandra.nex2;
	sumt;
end;
