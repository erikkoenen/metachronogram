begin mrbayes;
	set autoclose=yes nowarnings=yes;
[	set usebeagle=yes beagledevice=cpu beagleprecision=double;]
[	set beaglescaling=dynamic beaglesse=yes;]
        execute Zapoteca_clade_alignment.nex;
	lset nst=6 rates=gamma;
	constraint calliandra hard = Calliandra_dysantha Calliandra_surinamensis;
	constraint pithecellobium_clade hard = Havardia_mexicana Havardia_pallens Pithecellobium_diversifolium Pithecellobium_dulce;
	constraint zapoteca_clade hard = Faidherbia_ETS_ITS_trnL Sanjappa_cynometroides Thailentadopsis_nitida Thailentadopsis_tenuis Viguieranthus_ambongensis Viguieranthus_densinervus Viguieranthus_glaber Viguieranthus_kony Viguieranthus_megalophyllus Viguieranthus_subauriculatus Zapoteca_aculeata_K1 Zapoteca_alinae_K3 Zapoteca_amazonica_K30 Zapoteca_balsasensis_K7 Zapoteca_caracasana_subsp_caracasana_K34 Zapoteca_caracasana_subsp_weberbaueri_K32 Zapoteca_costaricensis_513 Zapoteca_cruzii_505 Zapoteca_filipes_isolate_FS05 Zapoteca_formosa_isolate_FS5607 Zapoteca_formosa_subsp_gracilis_K39 Zapoteca_formosa_subsp_mollicula_K13 Zapoteca_formosa_subsp_rosei_K15 Zapoteca_formosa_subsp_salvadorensis_K17 Zapoteca_formosa_subsp_schottii_K19 Zapoteca_lambertiana_K21 Zapoteca_media_K23 Zapoteca_mollis_K25 Zapoteca_nervosa_K40 Zapoteca_portoricensis_subsp_portoricensis_K42 Zapoteca_quichoi_498 Zapoteca_ravenii_K28 Zapoteca_scuttelifera_K29 Zapoteca_socorrensis_K20 Zapoteca_sousae_isolate_FS5613 Zapoteca_tehuana_339 Zapoteca_tetragona_isolate_FS5614;
	constraint pithecellobium_zapoteca_clades hard = Havardia_mexicana Havardia_pallens Pithecellobium_diversifolium Pithecellobium_dulce Faidherbia_ETS_ITS_trnL Sanjappa_cynometroides Thailentadopsis_nitida Thailentadopsis_tenuis Viguieranthus_ambongensis Viguieranthus_densinervus Viguieranthus_glaber Viguieranthus_kony Viguieranthus_megalophyllus Viguieranthus_subauriculatus Zapoteca_aculeata_K1 Zapoteca_alinae_K3 Zapoteca_amazonica_K30 Zapoteca_balsasensis_K7 Zapoteca_caracasana_subsp_caracasana_K34 Zapoteca_caracasana_subsp_weberbaueri_K32 Zapoteca_costaricensis_513 Zapoteca_cruzii_505 Zapoteca_filipes_isolate_FS05 Zapoteca_formosa_isolate_FS5607 Zapoteca_formosa_subsp_gracilis_K39 Zapoteca_formosa_subsp_mollicula_K13 Zapoteca_formosa_subsp_rosei_K15 Zapoteca_formosa_subsp_salvadorensis_K17 Zapoteca_formosa_subsp_schottii_K19 Zapoteca_lambertiana_K21 Zapoteca_media_K23 Zapoteca_mollis_K25 Zapoteca_nervosa_K40 Zapoteca_portoricensis_subsp_portoricensis_K42 Zapoteca_quichoi_498 Zapoteca_ravenii_K28 Zapoteca_scuttelifera_K29 Zapoteca_socorrensis_K20 Zapoteca_sousae_isolate_FS5613 Zapoteca_tehuana_339 Zapoteca_tetragona_isolate_FS5614;
	calibrate zapoteca_clade=fixed(10);
	prset topologypr=constraints(calliandra,pithecellobium_clade,zapoteca_clade,pithecellobium_zapoteca_clades);
	prset brlenspr=clock:uniform;
	prset clockvarpr=igr;
	showmodel;
	mcmc ngen=30000000 samplefreq=1000 file=zapoteca.nex1;
	mcmc file=zapoteca.nex2;
	sumt;
end;
